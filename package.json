{
  "name": "@osiris-smarttv/indexeddb-util",
  "version": "0.1.0",
  "description": "A TypeScript utility library for working with IndexedDB easily and efficiently",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "publish:npm": "npm publish",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "release:patch": "npm run version:patch && npm run publish:npm",
    "release:minor": "npm run version:minor && npm run publish:npm",
    "release:major": "npm run version:major && npm run publish:npm"
  },
  "keywords": [
    "indexeddb-util",
    "indexdb",
    "typescript",
    "performance",
    "memory-efficient",
    "smart-tv"
  ],
  "author": "Osiris TV",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-jest": "^29.7.0",
    "core-js": "^3.45.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fake-indexeddb": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OsirisTech-SmartTV/indexeddb-util.git"
  },
  "bugs": {
    "url": "https://github.com/OsirisTech-SmartTV/indexeddb-util/issues"
  },
  "homepage": "https://github.com/OsirisTech-SmartTV/indexeddb-util#readme",
  "browserslist": [
    "Chrome >= 53",
    "ChromeAndroid >= 53",
    "Edge >= 79",
    "Firefox >= 78",
    "Safari >= 13",
    "iOS >= 13"
  ]
}
